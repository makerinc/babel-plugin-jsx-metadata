{
  "name": "babel-plugin-jsx-metadata",
  "version": "1.0.0",
  "description": "Babel plugin that injects metadata into JSX elements for visual editor integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/generate-bridge-wrapper.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "vitest --watch & tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "babel",
    "plugin",
    "jsx",
    "metadata",
    "react"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
