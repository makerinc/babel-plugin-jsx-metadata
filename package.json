{
  "name": "babel-plugin-jsx-metadata",
  "version": "1.0.0",
  "description": "Babel plugin that injects metadata into JSX elements for visual editor integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "vitest --watch & tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "babel",
    "plugin",
    "jsx",
    "metadata",
    "react",
    "visual-editor",
    "component-tracking",
    "authorship"
  ],
  "author": "Your Name",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
